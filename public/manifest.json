{
  "name": "DeFi Core - Plataforma Financiera Descentralizada",
  "short_name": "DeFi Core",
  "description": "Plataforma completa de finanzas descentralizadas con préstamos, staking, remesas y más",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0f172a",
  "theme_color": "#3b82f6",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "es",
  "dir": "ltr",
  "categories": ["finance", "business", "productivity"],
  "icons": [
    {
      "src": "/src/assets/Logo.png",
      "sizes": "100x100",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/src/assets/Logo2.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Ver tu dashboard principal",
      "url": "/dashboard"
    },
    {
      "name": "Borrow",
      "short_name": "Borrow",
      "description": "Solicitar un préstamo",
      "url": "/borrow"
    },
    {
      "name": "Staking",
      "short_name": "Staking",
      "description": "Gestionar tu staking",
      "url": "/liquidity"
    },
    {
      "name": "Remittances",
      "short_name": "Remesas",
      "description": "Enviar remesas",
      "url": "/remittances"
    }
  ],
  "screenshots": [],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+defi",
      "url": "/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "handle_links": "preferred",
  "scope_extensions": [
    {
      "origin": "https://api.defi-core.com"
    }
  ]
}
