{
  "name": "banobs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "pnpm -r --filter '@core-neobank/api' dev & pnpm -r --filter '@core-neobank/keeper' dev",
    "build": "tsc -b && vite build",
    "build:all": "pnpm -r --filter '@core-neobank/api' build && pnpm -r --filter '@core-neobank/keeper' build && pnpm run build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest --run src/**/*.integration.test.{ts,tsx}",
    "test:contracts": "vitest --run src/**/*.contract.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:security": "npm audit && snyk test",
    "generate:abis": "node scripts/generate-abis.js",
    "generate-pwa-assets": "node scripts/generate-pwa-assets.js",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "pwa:audit": "npm run build && npm run preview & sleep 5 && npm run lighthouse",
    "pwa:install": "npm install sharp --save-dev",
    "pwa:setup": "npm run pwa:install && npm run generate-pwa-assets",
    "postinstall": "pnpm run generate:abis"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@sentry/react": "^10.5.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.85.0",
    "@web3modal/wagmi": "^5.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.0",
    "viem": "^2.33.3",
    "wagmi": "^2.16.3",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.8"
  }
}
